  "devDependencies": {
    "@nomiclabs/buidler": "1.1.2",
    "@nomiclabs/buidler-ethers": "1.1.2",
    "@nomiclabs/buidler-waffle": "1.1.2",
    "@types/chai": "4.2.9",
    "@types/mocha": "7.0.1",
    "@types/node": "13.7.1",
    "@types/sinon-chai": "3.2.3",
    "chai": "4.2.0",
