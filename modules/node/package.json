{
  "name": "nest-new",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config ops/nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@connext/nats-messaging-client": "^0.0.2",
    "@counterfactual/firebase-client": "0.0.3",
    "@counterfactual/node": "0.1.25",
    "@counterfactual/postgresql-node-connector": "0.0.2",
    "@counterfactual/types": "0.0.10",
    "@nestjs/common": "6.3.1",
    "@nestjs/core": "6.3.1",
    "@nestjs/platform-express": "6.3.1",
    "dotenv": "8.0.0",
    "ethers": "4.0.28",
    "firebase-server": "1.0.2",
    "pg": "7.11.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "rxjs": "6.5.2",
    "typeorm": "0.2.18",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "6.3.1",
    "@types/express": "4.17.0",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.6",
    "@types/supertest": "2.0.7",
    "concurrently": "4.1.0",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.2.0",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.5.1",
    "wait-on": "3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
